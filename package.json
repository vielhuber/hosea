{
    "config": {
        "domain": "http://hosea.local.vielhuber.de"
    },
    "scripts": {
        "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_public/_build/bundle.css",
        "css:sass:dev": "node-sass --source-map true ./_scss/style.scss ./_public/_build/bundle.css",
        "css:autoprefix": "postcss --use autoprefixer --output ./_public/_build/bundle.css ./_public/_build/bundle.css",
        "css": "npm-run-all --sequential css:sass css:autoprefix",
        "css:dev": "npm-run-all --sequential css:sass:dev",
        "js:browserify": "browserify ./_js/script.js --outfile ./_public/_build/bundle.js --debug --transform babelify --transform vueify",
        "js:uglify": "uglifyjs --compress --mangle --output ./_public/_build/bundle.js ./_public/_build/bundle.js",
        "js": "npm-run-all --sequential js:browserify js:uglify",
        "js:dev": "npm-run-all --sequential js:browserify",
        "html": "html-minifier --collapse-whitespace --output ./_public/index.html ./_html/index.html",
        "html:dev": "html-minifier --output ./_public/index.html ./_html/index.html",
        "browsersync": "cross-var browser-sync start --reload-debounce 3000 --proxy \"$npm_package_config_domain\" --files \"./_public/_build/*.css, ./_public/_build/*.js\"",
        "watch:css": "onchange ./_scss/**/*.scss --initial --wait --delay 200 --await-write-finish -- onerror \"npm run css:dev --silent\" --title \"watch:css\" --message \"build failed\" --sound mute",
        "watch:js": "onchange ./_js/**/*.js --initial --wait --delay 200 --await-write-finish -- onerror \"npm run js:dev --silent\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "watch:html": "onchange ./_html/**/*.html --initial --wait --delay 200 --await-write-finish -- onerror \"npm run html:dev --silent\" --title \"watch:html\" --message \"build failed\" --sound mute",
        "build": "npm-run-all --parallel html js css",
        "dev": "npm-run-all --parallel watch:* browsersync"
    },
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/runtime": "^7.4.5",
        "@prettier/plugin-php": "^0.11.2",
        "autoprefixer": "^9.6.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.8.0",
        "babel-plugin-array-includes": "^2.0.3",
        "babelify": "^10.0.0",
        "browser-sync": "^2.26.7",
        "browserify": "^16.2.3",
        "cli-error-notifier": "^2.1.0",
        "concat": "^1.0.3",
        "cpx": "^1.5.0",
        "critical": "^1.3.4",
        "cross-var": "^1.1.0",
        "eslint": "^5.16.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^6.1.0",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.2.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.7.0",
        "gulp-htmlmin": "^5.0.1",
        "gulp-jest": "^4.0.2",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "gulp-vueify": "0.0.3",
        "hlp": "^2.3.7",
        "html-minifier": "^4.0.0",
        "ismobilejs": "^0.5.2",
        "jest": "^24.8.0",
        "jest-cli": "^24.8.0",
        "jest-image-snapshot": "^2.8.2",
        "jest-puppeteer": "^4.2.0",
        "jwtbutler": "^1.5.3",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "onchange": "^6.0.0",
        "postcss-cli": "^6.1.2",
        "prettier": "^1.18.2",
        "puppeteer": "^1.17.0",
        "regenerator-runtime": "^0.13.2",
        "run-sequence": "^2.2.1",
        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "vueify": "^9.4.1",
        "vueify-insert-css": "^1.0.0",
        "whatwg-fetch": "^3.0.0"
    },
    "name": "hosea",
    "version": "1.0.0",
    "description": "",
    "main": "_public/_build/bundle.js",

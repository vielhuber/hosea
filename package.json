{
    "config": {
        "domain": "http://hosea-new.local"
    },
    "scripts": {
        "css:sass": "node-sass --source-map true --output-style compressed ./_scss/style.scss ./_public/_build/bundle.css",
        "css:autoprefix": "postcss --use autoprefixer --output ./_public/_build/bundle.css ./_public/_build/bundle.css",
        "css": "npm-run-all --sequential css:sass css:autoprefix",
        "js:browserify": "browserify ./_js/script.js --outfile ./_public/_build/bundle.js --debug --transform babelify --transform vueify",
        "js:uglify": "uglifyjs --compress --mangle --output ./_public/_build/bundle.js ./_public/_build/bundle.js",
        "js": "npm-run-all --sequential js:browserify js:uglify",
        "html": "html-minifier --collapse-whitespace --output ./_public/index.html ./_html/index.html",
        "browsersync": "cross-var browser-sync start --reload-debounce 3000 --proxy \"$npm_package_config_domain\" --files \"./_public/_build/*.css, ./_public/_build/*.js\"",
        "watch:css": "onchange ./_scss/**/*.scss --initial --wait --delay 1000 --await-write-finish -- \"npm run css\"",
        "watch:js": "onchange ./_js/**/*.js --initial --wait --delay 1000 --await-write-finish -- \"npm run js\"",
        "watch:html": "onchange ./_html/**/*.html --initial --wait --delay 1000 --await-write-finish -- \"npm run html\"",
        "build": "npm-run-all --parallel watch:*",
        "serve": "npm-run-all --parallel watch:* browsersync"
    },
    "dependencies": {
        "@prettier/plugin-php": "^0.6.0",
        "autoprefixer": "^9.1.5",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^23.4.2",
        "babel-plugin-array-includes": "^2.0.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "babelify": "^8.0.0",
        "browser-sync": "^2.24.7",
        "browserify": "^16.2.2",
        "concat": "^1.0.3",
        "cpx": "^1.5.0",
        "critical": "^1.3.4",
        "cross-var": "^1.1.0",
        "eslint": "^5.5.0",
        "fullcalendar": "^3.8.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^3.10.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.6.1",
        "gulp-htmlmin": "^4.0.0",
        "gulp-jest": "^4.0.2",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^3.0.1",
        "gulp-vueify": "0.0.3",
        "hlp": "^2.2.0",
        "html-minifier": "^3.5.20",
        "ismobilejs": "^0.4.1",
        "jest": "^23.4.2",
        "jest-cli": "^23.4.2",
        "jest-image-snapshot": "^2.5.0",
        "jest-puppeteer": "^3.3.1",
        "jquery": "^3.3.1",
        "jwtbutler": "^1.4.8",
        "moment": "^2.20.1",
        "node-sass": "^4.9.3",
        "npm-run-all": "^4.1.3",
        "onchange": "^4.1.0",
        "postcss-cli": "^6.0.0",
        "pouchdb": "^7.0.0",
        "prettier": "^1.14.2",
        "puppeteer": "^1.8.0",
        "regenerator-runtime": "^0.12.1",
        "run-sequence": "^2.2.1",
        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.5.17",
